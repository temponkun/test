const express = require('express');
const path = require('path');
const app = express();

app.set('/',(req,res)=>{
return res.send("OK");
})

app.set('view engine', 'pug');
app.set('views',path.join(__dirname,'views'));

app.get('/index',(req,res)=>{
res.render('index',{
});
});

app.listen(5000,()=>{
//title: 'test',
//name
//link
});
